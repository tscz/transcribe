{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": false,
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "eslint.format.enable": true,
    "eslint.packageManager": "yarn",
    "jest.pathToJest": "npm run testWithoutEnv --",
    "plantuml.diagramsRoot": "doc/diagrams/src",
    "plantuml.exportOutDir": "doc/diagrams/out"
}